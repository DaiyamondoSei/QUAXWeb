<invoke name="edit_file">
<parameter name="target_file">js/analytics.js</parameter>
<parameter name="instructions">I will update the analytics configuration to use the new domain name.</parameter>
<parameter name="code_edit">// Google Analytics Implementation
window.dataLayer = window.dataLayer || [];
function gtag(){dataLayer.push(arguments);}
gtag('js', new Date());
gtag('config', 'G-XXXXXXXXXX', {
    'cookie_domain': 'quannex.com',
    'linker': {
        'domains': ['quannex.com']
    }
});

// Enhanced Event Tracking
document.addEventListener('DOMContentLoaded', function() {
    // Track page views
    gtag('event', 'page_view', {
        'page_title': document.title,
        'page_location': window.location.href,
        'page_path': window.location.pathname
    });

    // Track CTA clicks
    document.querySelectorAll('.cta-button').forEach(button => {
        button.addEventListener('click', function() {
            gtag('event', 'cta_click', {
                'event_category': 'Engagement',
                'event_label': this.textContent.trim()
            });
        });
    });

    // Track feature exploration
    document.querySelectorAll('.feature').forEach(feature => {
        feature.addEventListener('click', function() {
            gtag('event', 'feature_click', {
                'event_category': 'Features',
                'event_label': this.querySelector('h3').textContent
            });
        });
    });

    // Track resource downloads
    document.querySelectorAll('.resource-card').forEach(card => {
        card.addEventListener('click', function() {
            gtag('event', 'resource_click', {
                'event_category': 'Resources',
                'event_label': this.querySelector('h3').textContent
            });
        });
    });
});</parameter>
</invoke> 